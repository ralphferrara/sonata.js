            "@types/ws": "^8.5.10",
            "@types/xmldom": "^0.1.34",
            "crypto": "^1.0.1",
            "fastify": "^4.25.2",
            "fastify-websocket": "^4.3.0",
            "jest": "^29.7.0",
            "jsdom": "^23.0.1",
            "mongodb": "^6.3.0",
            "mysql2": "latest",
            "redis": "^4.6.11",
            "sharp": "^0.32.6",
            "ts-jest": "^29.1.1",
            "ts-node": "^10.9.1",
            "typescript": "^5.3.3",
            "ws": "^7.5.9"
      },
      "dependencies": {
